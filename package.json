{
  "name": "mctl-ui",
  "version": "0.2.0",
  "private": false,
  "description": "react components library",
  "author": "gao shao hong",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "react",
    "component",
    "ui"
  ],
  "homepage": "https://github.com/mctl/mctl-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/mctl/mctl-ui"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 10",
    "build": "yarn clean && yarn build-ts && yarn build-css",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:nowatch": "cross-env CI=true react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepublishOnly": "yarn test:nowatch && yarn lint && yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test:nowatch && yarn lint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "antd": "^4.16.8",
    "axios": "^0.21.3",
    "classnames": "^2.2.6",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "marked": "^2.1.3",
    "react-transition-group": "^4.4.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-essentials": "^6.0.22",
    "@storybook/addon-links": "^6.0.22",
    "@storybook/node-logger": "^6.0.22",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.22",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-transition-group": "^4.4.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^4.3.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-scripts": "3.4.3",
    "rimraf": "^3.0.2",
    "typescript": "~3.8.0"
  }
}
